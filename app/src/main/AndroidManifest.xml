<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getjournal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GetJournal"
        android:usesCleartextTraffic="true">
        <activity android:name=".JurnalActivity"></activity>
        <activity android:name=".crud.AddEditActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".EditpassActivity" />
        <activity android:name=".EditprofileActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OnBoardActivity" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>